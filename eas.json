{
  "cli": {
    "version": ">= 0.52.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "env": {
        "APP_ENV": "development"
      },
      "android": {
        "buildType": "apk"
        // "applicationArchivePath": "dist/ConsultingApp-Dev-v1.0.1.apk"
      }
    },
    "staging": {
      "distribution": "internal",
      "env": {
        "APP_ENV": "staging"
        // "artifactPath": "dist/ConsultingApp-sta-v1.0.1.apk"
      },
      "channel": "staging"
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_ENV": "production"
      },
      "channel": "production",
      "android": {
        "buildType": "apk"
        // "applicationArchivePath": "dist/ConsultingApp-v1.0.1.apk"
      }
    },
    "preview": {  // ðŸ‘ˆ Agrega este perfil si falta
      "distribution": "internal",
      "env": {
        "APP_ENV": "development"
      },
      "channel": "preview",
      "android": {
        "buildType": "apk"
        // "applicationArchivePath": "dist/ConsultingApp-prev-v1.0.1.apk"
      }
    }
  
  }
}
